{
  "name": "myqa-backedn",
  "description": "The backend for myqa",
  "version": "0.1.0",
  "main": "src",
  "keywords": [
    "javascript",
    "myqa",
    "knowledge",
    "knowledge base"
  ],
  "license": "MIT",
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "check-types": "tsc --noEmit --skipLibCheck",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts",
    "lint-fix": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "lint-staged": "lint-staged",
    "dev": "ts-node-dev --trace-warnings -r tsconfig-paths/register --no-notify src/",
    "start": "npm run build && node dist/src",
    "serve": "node dist/src",
    "build": "shx rm -rf dist/ && ttsc --p ./tsconfig.build.json --outDir dist",
    "prepare": "husky install",
    "test": "jest --verbose",
    "pre-commit": "npm run test && npm run lint-staged && npm run check-types"
  },
  "lint-staged": {
    "**/*.ts": "eslint"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.38",
    "@types/config": "0.0.41",
    "@types/jest": "^29.5.0",
    "@types/koa__cors": "^4.0.0",
    "@types/koa__multer": "^2.0.4",
    "@types/koa-bodyparser": "^4.3.5",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-passport": "^6.0.0",
    "@types/koa-router": "^7.4.4",
    "@types/lodash": "^4.14.179",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/semver": "^7.3.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^7.32.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^12.3.4",
    "shx": "^0.3.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.12.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.11.2",
    "@koa/cors": "^4.0.0",
    "@koa/multer": "^3.0.2",
    "bcrypt": "^5.1.0",
    "bluebird": "^3.7.2",
    "config": "^3.3.1",
    "form-data": "^4.0.0",
    "json-schema-to-ts": "^2.7.2",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.4.0",
    "koa-cookie": "^1.0.0",
    "koa-logger": "^3.2.1",
    "koa-passport": "^6.0.0",
    "koa-router": "^10.1.1",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "mysql2": "^3.2.0",
    "node-fetch": "^2.6.9",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.13",
    "typeorm-extension": "^2.7.0",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1"
  }
}
